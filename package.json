{
  "name": "dotpath-resolve",
  "description": "Dot-path based object property selector",
  "author": "Aleksandr Tsertkov <tsertkov@gmail.com>",
  "version": "0.1.2",
  "main": "lib/resolve.js",
  "license": {
    "type": "MIT",
    "url": "https://github.com/tsertkov/dotpath-resolve/blob/master/LICENSE-MIT"
  },
  "bugs": {
    "url": "https://github.com/tsertkov/dotpath-resolve/issues"
  },
  "scripts": {
    "test": "npm run cover-test && npm run jshint",
    "cover-test": "istanbul cover --dir ./artifacts/coverage test/*",
    "jshint": "jshint lib/ test/ example/ package.json",

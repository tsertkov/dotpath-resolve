  "repository": {
    "type": "git",
    "url": "git@github.com:tsertkov/dotpath-resolve.git"
  },
  "keywords": [
    "walk",
    "traverse",
    "dotpath",
    "dot-path",
    "resolve",
    "object",
    "selector"
  ],
  "devDependencies": {
    "coveralls": "^2.11.2",
    "istanbul": "^0.3.2",
    "jsdoc": "^3.3.0-alpha9",
    "jshint": "^2.5.5",
    "nodemon": "^1.2.1"
  }
}
